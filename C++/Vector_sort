#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    vector<int>v;                // Declaration of vector
    int n,x;
    cin>>n;
    for(int i=0;i<n;i++)
    {
    cin>>x;
    v.push_back(x);             // To push the elements in vector from last
    }
    sort(v.begin(),v.end());    // Function to sort the vector
    for(int i=0;i<n;i++)
    {
        cout<<v[i]<<" ";
    } 
    return 0;
}
